    "    #Tokenize\n",
    "    tokenizer= RegexpTokenizer(r\"\\w+\")\n",
    "    raw_tokens_list= tokenizer.tokenize(doc)\n",
    "    \n",
    "    # classics stop_words\n",
    "    cleaned_tokens_list= [w for w in raw_tokens_list if w not in stop_words]\n",
