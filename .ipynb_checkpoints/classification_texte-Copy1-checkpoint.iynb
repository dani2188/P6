    "    else: \n",
    "        alpha_tokens = more_than_N\n",
    "          \n",
    "    ###############################\n",
    "    \n",
    "    if rejoin: \n",
    "        return \" \".join(alpha_tokens)\n",
    "    \n",
    "    return alpha_tokens"
   ]
